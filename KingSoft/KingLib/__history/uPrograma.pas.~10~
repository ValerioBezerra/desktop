unit uPrograma;

interface

uses
   uModulo;

type
  TPrograma = class
  private
    FId: Integer;
    FModulo: TModulo;
    FCodigo: String;
    FDescricao: String;
    FMenuPrincipal: Boolean;
    FVisualizar: Boolean;
    FIncluir: Boolean;
    FEditar: Boolean;
    FApagar: Boolean;
  public
    property Id: Integer read FId write FId;
    property Modulo: TModulo read FModulo write FModulo;
    property Codigo: String read FCodigo write FCodigo;
    property Descricao: String read FDescricao write FDescricao;
    property MenuPrincipal: Boolean read FMenuPrincipal write FMenuPrincipal;
    property Visualizar: Boolean read FIncluir write FIncluir;
    property Incluir: Boolean read FIncluir write FIncluir;
    property Editar: Boolean read FEditar write FEditar;
    property Apagar: Boolean read FApagar write FApagar;
end;

implementation

{ TPrograma }

end.

