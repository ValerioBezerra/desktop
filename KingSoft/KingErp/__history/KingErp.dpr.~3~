program KingErp;

uses
  Vcl.Forms,
  uLogin in 'uLogin.pas' {frmLogin},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Sapphire Kamri');
  Application.CreateForm(TfrmLogin, frmLogin);
  Application.Run;
end.
